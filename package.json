{
    "name": "quickstart-template-angular-helloworld-client",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "config": "node ./src/set-env.js",
        "start": "npm run config && ng serve -o --host 0.0.0.0",
        "build": "npm run config && ng build",
        "build:dev": "npm run config && ng build --c=production --aot=true --base-href ${RELATIVE_URL}",
        "watch": "ng build --watch --configuration development",
        "test": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
        "lint": "ng lint",
        "lint:fix": "ng lint --fix=true",
        "test:coverage": "ng test --code-coverage",
        "pre-commit": "lint-staged",
        "prepare": "husky install"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.2.0",
        "@angular/cdk": "^13.2.4",
        "@angular/common": "~13.2.0",
        "@angular/compiler": "~13.2.0",
        "@angular/core": "~13.2.0",
        "@angular/forms": "~13.2.0",
        "@angular/material": "^13.2.4",
        "@angular/platform-browser": "~13.2.0",
        "@angular/platform-browser-dynamic": "~13.2.0",
        "@angular/router": "~13.2.0",
        "dotenv": "^16.0.0",
        "ngx-pagination": "^6.0.3",
        "oidc-client": "^1.11.5",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.2.3",
        "@angular-eslint/builder": "13.1.0",
        "@angular-eslint/eslint-plugin": "13.1.0",
        "@angular-eslint/eslint-plugin-template": "13.1.0",
        "@angular-eslint/schematics": "13.1.0",
        "@angular-eslint/template-parser": "13.1.0",
        "@angular/cli": "~13.2.3",
        "@angular/compiler-cli": "~13.2.0",
        "@types/jasmine": "~3.10.0",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "5.11.0",
        "@typescript-eslint/parser": "5.11.0",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "jasmine-core": "~4.0.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "lint-staged": "^12.3.4",
        "typescript": "~4.5.2"
    }
}
